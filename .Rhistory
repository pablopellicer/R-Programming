pollutantmean <- function(directory, pollutant, id = 1:332) {
currentwd<-getwd()
setwd(directory)
round<-0
files <- list.files()
for (i in id) {
if (round==0) {
data <- read.csv(files[i], header=TRUE)
round<-1
} else {
data_tmp <- read.csv(files[i], header=FALSE)
names(data_tmp) <- names(data)
data <- rbind(data,data_tmp)
}
}
setwd(currentwd)
